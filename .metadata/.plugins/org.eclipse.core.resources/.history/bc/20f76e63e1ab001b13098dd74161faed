public class Solution {
	public static void main(String[] args) {
		int s = 0;
		int[] input = {0, 4, 8, 11, 2, 5, 14, 6};
		int[][] a = {{0, 3, 2, 4, 1}, {4, 5, 1, 3, 2}, {1, 2, 4, 5, 1}};
		for(int n = 1; n <= 7; n++) {
			int v = input[n];
			int r = v / 5;
			int c = v % 5;
			s = s + a[r][c];
		}
		System.out.println(s);
	}
}