public class Solution {
	public static void main(String[] args) {
		String a$ = "thewizardingworldofharrypotter";
		String b$ = "";
		String c$ = "";
		String g$ = "";
		String p$ = "";
		for(int i = 0; i < a$.length(); i++) {
			char d$ = a$.charAt(i);
			if(d$ - 'a' < 'm' - 'a')
				b$ += d$ + "";
			else
				c$ += d$ + "";
		}
	}
}