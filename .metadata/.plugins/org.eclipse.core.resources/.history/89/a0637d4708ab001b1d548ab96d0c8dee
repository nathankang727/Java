import java.util.*;
public class Solution {
	public static void main(String[] args) {
		String a = "FOURTHACSLCONTEST";
		String b = "";
		for(int x = 1; x <= a.length() - 2; x++) {
			if(a.charAt(x) - 'A' > 'O' - 'A')
				b = b + a.charAt(x + 1);
			if(a.charAt(x) - 'A' < 'O' - 'A')
				b = a.charAt(x - 1) + b;
		}
		System.out.println(b);
		int n = 2;
		for(int i = 0; i <= a.length() - n; i++) {
			char[] str = new char[n];
			char[] str2 = new char[n];
			int index = 0;
			for(int j = i; j < i + n; j++) {
				str[index] = b.charAt(j);
				str2[index] = b.charAt(j);
				++index;
			}
			Arrays.sort(str2);
			System.out.println(Arrays.toString(str) + " " + Arrays.toString(str2));
			if(str.equals(str2))
				System.out.println("YEEE");
		}
	}
}